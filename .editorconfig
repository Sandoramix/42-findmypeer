[*.{ts,tsx}]
indent_style = tab
indent_size = 4
insert_final_newline = false
trim_trailing_whitespace = true
quote_type = backtick

[*.md]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.json]
indent_style = tab
indent_size = 4

[package.json]
indent_style = tab
indent_size = 4